extends layout.pug

block content
    h1 Portfolio Page
    .users 
        if users 
            each user in users
                .user-box 
                    h3 #{user.name}
                    p #{user.email}
                    p #{user.phone}
                    img(src=user.profileImage, width="150", height="150" alt="", srcset="")
                    a(href='/user/' + user._id) Edit User 